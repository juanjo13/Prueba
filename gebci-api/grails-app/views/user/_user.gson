import mx.gob.fgjez.gebci.security.User

model {
    User user
}

json {
    username user.username
    rol user.authorities.find{ a -> a.authority != 'ROLE_USER' }.authority
    nombre "${[user.details?.nombre, user.details?.paterno, user.details?.materno].join(' ').trim()}"
    email user.details?.email
}
